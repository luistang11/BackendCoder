<h1>Productos</h1>
<div>
  {{#each docs}}
    <p>id: {{_id}}</p>
    <p>title: {{title}}</p>
    <p>description: {{description}}</p>
    <p>code: {{code}}</p>
    <p>price: {{price}}</p>
    <p>status: {{status}}</p>
    <p>stock: {{stock}}</p>
    <p>thumbnail: {{thumbnail}}</p>
    <h2>----------</h2>
  {{/each}}
</div>

{{#if prevPage}}
  <a href="/pagination?page={{prevPage}}">Anterior</a>
{{/if}}
{{page}}
{{#if nextPage}}
  <a href="/pagination?page={{nextPage}}">Siguiente</a>
{{/if}}